{
  "env": {
    "browser": true,
    "es6": false, // TODO: remove in AIRFLOW-2804 once all JS is processed by Babel
    "jquery": true
  },
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "module"
  },
  "plugins": [
    "@jupyterlab/eslint-plugin-jinja",
    "html"
  ],
  "rules": {
     // TODO: remove these rule overrides once the codebase is migrated to fully ES6
    "func-names": "off",
    "object-shorthand": "off",
    "prefer-arrow-callback": "off"
  }
}
